Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"D", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Sqrt", "[", "x", "]"}], 
    RowBox[{"Tanh", "[", 
     RowBox[{"Sin", "[", "x", "]"}], "]"}]}], ",", "x"}], "]"}]], "Input",
 CellChangeTimes->{{3.7636871050724497`*^9, 
