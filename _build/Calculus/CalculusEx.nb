Cell[BoxData[
 RowBox[{
  RowBox[{
   SqrtBox["x"], " ", 
   RowBox[{"Cos", "[", "x", "]"}], " ", 
   SuperscriptBox[
    RowBox[{"Sech", "[", 
     RowBox[{"Sin", "[", "x", "]"}], "]"}], "2"]}], "+", 
  FractionBox[
   RowBox[{"Tanh", "[", 
    RowBox[{"Sin", "[", "x", "]"}], "]"}], 
   RowBox[{"2", " ", 
    SqrtBox["x"]}]]}]], "Output",
